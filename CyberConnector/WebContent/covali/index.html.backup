                          <div class="item"><span class="label">Mime Type: </span><span class="value">{{dataColumn.mime_type}}</span></div>
                        </div>
                      </div>
                      {{/dataColumn}}
                    </div>
                    {{/columns}}
                  </div>
                </div>
              </div>
              <div class="features" style="display: none;">
              </div>
            </div>
            <hr>
          </div>
